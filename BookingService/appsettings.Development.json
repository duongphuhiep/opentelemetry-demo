{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Properties": {
        "ApplicationName": "booking-service",
        "Env": "local",
        "logType": "booking-service"
    },
    "Using": [
        "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
        "Default": "Debug",
        "Override": {
            "Microsoft": "Warning",
            "System": "Warning"
        }
    },
    "Enrich": [ 
      "FromLogContext", 
      "WithMachineName", 
      "WithProcessId", 
      "WithThreadId", 
      "WithExceptionDetails",
      "WithClientIp", 
      "WithCorrelationId",
      { 
          "Name": "WithRequestHeader", 
          "Args": { "headerName": "User-Agent"}
      }
  ],
    "WriteTo": [
        { "Name": "Console" },
        {
            "Name": "Seq",
            "Args": {
                "ServerUrl": "http://localhost:5341"
            }
        }
    ]
},
  "CarRentalServiceUri": "https://localhost:7218",
  "OtlpExporterGrpcUri": "http://localhost:4317"
}
